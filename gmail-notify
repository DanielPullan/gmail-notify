import feedparser




username = "username@gmail.com"

password = "password"



mail = int(feedparser.parse("https://" + username + ":" + password + "@mail.google.com/gmail/feed/atom/")["feed"] ["fullcount"])

contact = int(feedparser.parse("https://" + username + ":" + password + "@mail.google.com/gmail/feed/atom/dan")["feed"] ["fullcount"])


if mail > 0 and \
contact > 0:
	LedBorg = open('/dev/ledborg', 'w')

	LedBorg.write('020')

	del LedBorg

	print ('You have a message from contact')

elif mail > 4:
	LedBorg = open('/dev/ledborg', 'w')

	LedBorg.write('200')

	del LedBorg

	print ('You have too many messages in your inbox')

elif mail > 0:
	LedBorg = open('/dev/ledborg', 'w')

	LedBorg.write('002')

	del LedBorg

	print ('You have a message in your inbox')

else:	
	LedBorg = open('/dev/ledborg', 'w')

	LedBorg.write('000')

	del LedBorg

	print ('Nope, no mail yet!')
